<?php
/**
 * Layouts/Blog default view
 *
 * @package photolab
 */
?><?php for($i = 0; $i < count($posts); $i++): ?>
	<?php $post = $posts[ $i ]; ?>
	<?php setup_postdata( $post ); ?>
	<?php echo View::make( 'contents/'.Blog_Settings_Model::getContentName() , Utils::array_except(get_defined_vars(), array('__data', '__path'))); ?>
<?php endfor; ?>
